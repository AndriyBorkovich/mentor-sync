---
extends: default
rules:
    braces:
        level: error
        max-spaces-inside: 1
    brackets:
        level: error
        max-spaces-inside: 1
    line-length:
        max: 160
        level: warning
    indentation:
        level: error
        indent-sequences: consistent
    trailing-spaces: disable
    document-start: disable
    truthy: disable
    comments-indentation: disable
